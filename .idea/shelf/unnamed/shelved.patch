+package app.controller.service.impl;
+
+import app.dao.UserDao;
+import app.dao.impl.UserDatabaseDao;
+import app.exception.ServiceException;
+import app.model.User;
+import app.controller.service.UserService;
+
+import java.util.List;
+
+public class UserServiceImpl implements UserService
+{
+
+    private UserDao userDao = new UserDatabaseDao();
+
+    @Override
+    public boolean add(User user) throws ServiceException {
+        try {
+            return userDao.add(user);
+        }catch (Exception ex)
+        {
+            throw new ServiceException();
+        }
+    }
+
+    @Override
+    public User update(List<User> users)
+    {
+            return userDao.update(users);
+    }
+
+    @Override
+    public boolean delete(User user) {
+
+               return userDao.delete(user);
+
+    }
+
+    @Override
+    public User findById(Integer id) throws ServiceException {
+        try {
+            return userDao.findById(id);
+        }catch (Exception ex)
+        {
+            System.out.println(String.format("Cannot find user by id = ?",id));
+            throw new ServiceException();
+        }
+    }
+
+    @Override
+    public User findByMailAndPassword(String mail,String pass) throws ServiceException {
+        try {
+            return userDao.findByMailAndPassword(mail,pass);
+        }catch (Exception ex)
+        {
+            System.out.println(String.format("Cannot find user by mail = ?",mail));
+            throw new ServiceException();
+        }
+    }
+
+    @Override
+    public List<User> findAll()
+    {
+            return userDao.findAll();
+    }
+}
